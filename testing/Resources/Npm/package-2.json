{
  "name": "Sample Package Json 02",
  "version": "0.0.0",
  "license": "MIT",
  "main": ".vite/build/app.electron.js",
  "productName": "WNG Development Tools",
  "description": "Sample Package Testing 02",
  "scripts": {
    "ng": "ng",
    "<-- -------------|>   START   <|------------- -->": "",
    "start": "npm run dev",
    "serve": "ng serve wng-app",
    "dev": "concurrently -k \"ng serve wng-app\" \"npm run start-url\"",
    "dbg": "concurrently -k \"ng serve wng-app\" \"npm run start-dbg\"",
    "prod": "npm run build && npm run start-prd",
    "release": "@powershell -NoProfile -ExecutionPolicy Bypass -Command ./wng.ps1 -command release",
    "<-- -------------|>   MODES   <|------------- -->": "",
    "start-dbg": "wait-on http://localhost:4400",
    "start-url": "wait-on http://localhost:4400 && npm run start-dev",
    "start-dev": "electron-forge start -- -- NODE_APP_ENV=development --NODE_ENV=development",
    "start-prd": "electron-forge start -- -- NODE_APP_ENV=production --NODE_ENV=production",
    "<-- -------------|>   FORGE   <|------------- -->": "",
    "package": "electron-forge package -- -- NODE_APP_ENV=production --NODE_ENV=production",
    "make": "electron-forge make -- -- NODE_APP_ENV=production --NODE_ENV=production",
    "publish": "electron-forge publish",
    "lint": "npx eslint ./projects/**/*.ts",
    "<-- -------------|>   BUILD   <|------------- -->": "",
    "build": "ng build wng-app --configuration production",
    "build:app": "ng build wng-app --configuration production",
    "build:common": "ng build wng-common --configuration production",
    "vite:build": "vite build --config vite.main.config.ts",
    "<-- -------------|>   CLEAR   <|------------- -->": "",
    "clear": "rimraf ./dist ./.vite ./.angular",
    "clear:full": "rimraf ./dist ./.angular ./.vite ./node_modules ./package-lock.json ./projects/wng-common/package-lock.json ./projects/wng-common/node_modules"
  },
  "prettier": {
    "printWidth": 1000,
    "singleQuote": false,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/common": "^21.0.6",
    "@angular/compiler": "^21.0.6",
    "@angular/core": "^21.0.6",
    "@angular/forms": "^21.0.6",
    "@angular/platform-browser": "^21.0.6",
    "@angular/platform-browser-dynamic": "^21.0.6",
    "@angular/router": "^21.0.6",
    "@codemirror/lang-angular": "^0.1.4",
    "@codemirror/lang-javascript": "^6.2.4",
    "@codemirror/lang-json": "^6.0.2",
    "@codemirror/lang-sql": "^6.10.0",
    "@codemirror/theme-one-dark": "^6.1.3",
    "@date-fns/tz": "^1.4.1",
    "@fsegurai/codemirror-theme-nord": "^6.2.2",
    "@fsegurai/codemirror-theme-palenight": "^6.2.2",
    "@fsegurai/codemirror-theme-tokyo-night-storm": "^6.2.2",
    "@handsontable/angular": "^16.0.1",
    "@lezer/common": "^1.2.3",
    "@lezer/highlight": "^1.2.1",
    "@lezer/lr": "^1.4.2",
    "@primeng/themes": "^21.0.2",
    "@replit/codemirror-indentation-markers": "^6.5.3",
    "@replit/codemirror-lang-csharp": "^6.2.0",
    "codemirror": "^6.0.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "electron-squirrel-startup": "^1.0.1",
    "filesize": "^11.0.2",
    "handlebars": "^4.7.8",
    "jquery": "^3.7.1",
    "mssql": "^11.0.1",
    "node-machine-id": "^1.1.12",
    "primeng": "^21.0.2",
    "rxjs": "~7.8.2",
    "slickgrid": "^5.15.5",
    "sortablejs": "^1.15.6",
    "sqlite": "^5.1.1",
    "sqlite3": "^1.1.7",
    "sweetalert2": "^11.22.4",
    "tedious": "^18.6.1",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^21.0.4",
    "@angular/cli": "^21.0.4",
    "@angular/compiler-cli": "^21.0.6",
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-squirrel": "^7.8.3",
    "@electron-forge/maker-zip": "^7.8.3",
    "@electron-forge/plugin-auto-unpack-natives": "^7.8.3",
    "@electron-forge/plugin-fuses": "^7.8.3",
    "@electron-forge/plugin-vite": "^7.8.3",
    "@electron/fuses": "^1.8.0",
    "@electron/packager": "^18.4.4",
    "@types/electron-squirrel-startup": "^1.0.2",
    "@types/jasmine": "~5.1.0",
    "@types/jquery": "^3.5.32",
    "@types/mocha": "^10.0.10",
    "@types/mssql": "^9.1.7",
    "@types/sortablejs": "^1.15.8",
    "concurrently": "^9.2.0",
    "electron": "^37.3.0",
    "globals": "^16.3.0",
    "replace-in-file": "^8.3.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.9.3",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
