    "update": "npm run clear:full && npm run sync && npm install --legacy-peer-deps && npm run build",
    "build": "npm run clear && npm run webpack:vendor && npm run webpack:dist",
    "build:prod": "npm run clear && npm run webpack:vendor-prod && npm run webpack:dist-prod",
    "sync:all": "npm run sync && links.sync.bat",
    "sync": "dotnet msbuild /t:CopyLinkedContentFiles",
    "clear": "npm.clear.bat",
    "clear:full": "npm.clear.full.bat && dotnet restore",
    "install:fresh": "npm run clear:full && npm install --legacy-peer-deps",
    "webpack:dist": "node --openssl-legacy-provider --max_old_space_size=10192 --max_semi_space_size=2 node_modules/webpack/bin/webpack.js --config webpack.config.dev.js --progress --color",
    "webpack:vendor": "node --openssl-legacy-provider --max_old_space_size=10192 --max_semi_space_size=2 node_modules/webpack/bin/webpack.js --config webpack.config.vendor.dev.js --progress --color",
    "webpack:dist-prod": "node --openssl-legacy-provider --max_old_space_size=10192 --max_semi_space_size=2 node_modules/webpack/bin/webpack.js --config webpack.config.js",
    "webpack:vendor-prod": "node --openssl-legacy-provider --max_old_space_size=10192 --max_semi_space_size=2 node_modules/webpack/bin/webpack.js --config webpack.config.vendor.js"
  },
