{
  "name": "Sample Package Json 01",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "|>start": "<-- -------------------------------------- -->",
    "start": "npm run clear && run-script-os",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng serve --port 4500 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng serve --port 4500 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "|>build": "<-- -------------------------------------- -->",
    "build": "npm run clear && ng build",
    "build:production": "ng build --configuration production",
    "clear": "rimraf ./dist",
    "check:circular": "npx madge --circular --warning --extensions ts ./"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.0.6",
    "@angular/cdk": "^21.0.5",
